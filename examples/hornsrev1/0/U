FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

#include        "ASLparameters"

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform $flowVelocity;

boundaryField
{
    outlet
    {
		type 			zeroGradient;
    }

    inlet
    {
        type            fixedValue;
	value           uniform $flowVelocity;
    }
	
    #include "sideBottomTopPatches"
}

